{
    "name": "react-webpack-boilerplate",
    "version": "1.0.0",
    "description": "React boilerplate describing the ideal React workflow of Joshua Uzzell as of January 2023. (Workflow subject to evolution).",
    "main": "src/app/index.html",
    "scripts": {
        "build": "webpack --env env=dev --mode development",
        "deploy:prod": "webpack --env env=prod--mode production && deploy/scripts/_deploy.sh",
        "deploy:test": "webpack --env env=test --mode production && deploy/scripts/_deploy.sh"
    },
    "keywords": [],
    "author": "Joshua Uzzell <josh.uzzell@gmail.com>",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/plugin-transform-modules-commonjs": "^7.20.7",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "babel-loader": "^9.1.2",
        "css-loader": "^6.7.3",
        "html-loader": "^4.2.0",
        "html-webpack-plugin": "^5.5.0",
        "sass": "^1.57.1",
        "sass-loader": "^13.2.0",
        "style-loader": "^3.3.1",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
